{
  "name": "asist-preceptores",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "proxy": "http:127.0.0.1:3001",
  "workspaces": [
    "pages/*"
  ],
  "scripts": {
    "server": "node ./src/back/server.js",
    "dev": "vite",
    "build": "vite build",
    "build-all": "npm-run-all --parallel build:asistenter build:index build:login build:register //build:account build:asistencias build:blog",
    "build-freq": "npm-run-all --parallel build:asistenter build:index build:login build:register",
    "build:asistenter": "cd pages/asistenter && npm run build",
    "build:index": "cd pages/index && npm run build",
    "build:login": "cd pages/login && npm run build",
    "build:register": "cd pages/register && npm run build",
    "build:account": "cd pages/account && npm run build",
    "build:asistencias": "cd pages/asistencias && npm run build",
    "build:blog": "cd pages/blog && npm run build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-validator": "^7.2.0",
    "mysql": "^2.18.1",
    "mysql2": "^3.11.0",
    "nodemon": "^3.1.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "sequelize": "^6.37.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "npm-run-all": "^4.1.5",
    "sass-embedded": "^1.79.4",
    "vite": "^5.4.8"
  }
}
